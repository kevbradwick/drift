{
  "name": "kevbradwick/drift",
  "description": "A lightweight PHP library for mapping data to PHP classes",
  "type": "library",
  "keywords": ["mapper", "data", "data mapper"],
  "homepage": "https://github.com/kevbradwick/drift",
  "license": "MIT",
  "authors": [
    {
      "name": "Kevin Bradwick",
      "email": "kevinbradwick@gmail.com"
    }
  ],
  "support": {
    "source": "https://github.com/kevbradwick/drift",
    "issues": "https://github.com/kevbradwick/drift/issues"
  },
  "autoload": {
    "psr-4": {
      "Drift\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "fixtures\\": "test/fixtures/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "~5.2.1",
    "squizlabs/php_codesniffer": "~2.5.1"
  },
  "require": {
    "php": ">=5.6"
  }
}
